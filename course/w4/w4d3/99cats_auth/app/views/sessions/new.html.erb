<form action="/session" method="post">
  <% auth_token(form_authenticity_token) %>

  <label for="session_user_name"> Username </label><br>
  <input type="text" name="user[user_name]"><br>

  <label for= "session_password"> Password </label><br>
  <input type="text" name="user[password]"><br>

  <button> Submit </button> <br>
</form>

<% content_for :footer do %>
  <p>This is the new session form aka login page.</p>
<% end %>
